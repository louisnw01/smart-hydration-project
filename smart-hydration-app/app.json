{
  "expo": {
    "name": "smart-hydration-app",
    "slug": "smart-hydration-app",
    "owner": "smart-hydration-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": {
      "image": "./assets/water-drop-svgrepo-com.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/water-drop-svgrepo-com.svg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.louisnw01.smarthydrationapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.louisnw01.smarthydrationapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "429f1ddf-7110-4380-a62a-6f3856a4a1dc"
      }
    }
  }
}
